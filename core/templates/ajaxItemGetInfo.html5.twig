{% if objItem.socket0 != 0 %}
    <div style='position: relative; margin-bottom: 4px;'>
        <img src='../../images/case.svg'style='border: 1px solid #6b645b;' width='32' height='32' />
        {% if objItem.socket0 != 1 %}
            <span >{{ objItem.socket0Name }}</span>
            <img src='{{ objItem.socket0Chemin }}' style='display: inline; position: absolute; top:0px; left:0px; cursor: default; width: 32px;' />
        {% else %}
            <span >Emplacement libre</span>
        {% endif %}
    </div>
{% endif %}

