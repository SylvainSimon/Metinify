<br/>
{% set bonusBase1 = objItem.applytype0 %}
{% if bonusBase1 != 0 %}
    <br/>
    {% if objItem.applyvalue0 < 0 %}
        <span class='text-red'>{{ helpItemAttr(bonusBase1) }} : {{ objItem.applyvalue0 }}</span>
